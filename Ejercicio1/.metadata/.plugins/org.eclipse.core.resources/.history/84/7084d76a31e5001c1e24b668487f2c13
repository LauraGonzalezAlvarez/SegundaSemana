
/**
 * Esta clase contiene los valores de Júpiter
 *
 * @author Gretty Mosquera <gretymosquera@gmail.com>
 * @version 1.0.0 2022-04-18
 * @since 1.0.0
 */
public class Jupiter extends Planet {
    /*
     * Constructor
     * Inicializa los atributos de Júpiter
     */
    public Jupiter() {
        super("Júpiter", 1.90, 4.7, 7.4, 1.5);
    }

    @Override
    public String informationPlanet() {
        return "Identificador: " + getID() + "\n" +
                "Nombre: " + getName() + "\n" +
                "Masa: " + getMass() + "\n" +
                "Densidad: " + getDensity() + "\n" +
                "Diametro: " + getDiameter() + "\n" +
                "Distancia: " + getDistance() + "\n";
    }
}