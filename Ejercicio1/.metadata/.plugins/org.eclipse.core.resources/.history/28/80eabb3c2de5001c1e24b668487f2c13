/**
 * Esta clase contiene los valores de Marte
 *
 * @author Gretty Mosquera <gretymosquera@gmail.com>
 * @version 1.0.0 2022-04-19
 * @since 1.0.0
 */
public class Mars extends Planet {
    /*
     * Constructor
     * Inicializa los atributos de Marte.
     */
    public Mars() {
        super("Marte", 6.40, 2.7, 3.8, 4.6);
    }

    @Override
    public String viewInformationAsString() {
        return "Identificador: " + getID() + "\n" +
                "Nombre: " + getName() + "\n" +
                "Masa: " + getMass() + "\n" +
                "Densidad: " + getDensity() + "\n" +
                "Diametro: " + getDiameter() + "\n" +
                "Distancia: " + getDistance() + "\n";
    }
}