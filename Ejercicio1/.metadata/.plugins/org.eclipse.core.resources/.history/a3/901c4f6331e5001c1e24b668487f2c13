
/**
 * Esta clase contiene los valores de Urano
 *
 * @author Gretty Mosquera <gretymosquera@gmail.com>
 * @version 1.0.0 2022-04-20
 * @since 1.0.0
 */
public class Uranus extends Planet {
    /*
     * Constructor
     * Inicializa los atributos de Urano.
     */
    public Uranus() {
        super("Urano", 8.67, 4.7, 2.5, 5.6);
    }

    @Override
    public String informationPlanet() {
        return "Identificador: " + getID() + "\n" +
                "Nombre: " + getName() + "\n" +
                "Masa: " + getMass() + "\n" +
                "Densidad: " + getDensity() + "\n" +
                "Diametro: " + getDiameter() + "\n" +
                "Distancia: " + getDistance() + "\n";
    }
}