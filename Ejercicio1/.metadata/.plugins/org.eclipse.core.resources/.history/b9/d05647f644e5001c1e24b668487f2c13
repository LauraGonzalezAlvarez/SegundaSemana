
/**
 * Esta clase contiene la información de camión
 *
 * @author Gretty Mosquera <gretymosquera@gmail.com>
 * @version 1.0.0 2022-04-24
 * @since 1.0.0
 */
public class Truck extends Vehicle {
    /**
     * Atributos
     */

    private String truckType;

    /**
     * Métodos setters
     */
    public void setTruckType(String truckType) {
        this.truckType = truckType;
    }

    /**
     * Métodos getters
     */
    public String getTruckType() {
        return this.truckType;
    }
    /**
     * @return String
     * @author Gretty Mosquera <gretymosquera@gmail.com>
     * @since 1.0.0
     */
    public String startUp() {
        return "";
    }

    /**
     * @return String
     * @author Gretty Mosquera <gretymosquera@gmail.com>
     * @since 1.0.0
     */
    public String stop() {
        return "";
    }

    @Override
    public String toString(){
        return "Tipo de vehículo: Camión \n" +
                super.toString() + " \n Tipo: " + (getTruckType());
    }
}